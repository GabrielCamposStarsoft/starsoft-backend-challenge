{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "setupFiles": ["<rootDir>/setup-integration.ts"],
  "testEnvironment": "node",
  "testRegex": "\\.integration-spec\\.ts$",
  "transform": {"^.+\\.(t|j)s$": "ts-jest"},
  "moduleNameMapper": {"^src/(.*)$": "<rootDir>/../src/$1"},
  "testTimeout": 30000,
  "verbose": true,
  "maxWorkers": 1
}
